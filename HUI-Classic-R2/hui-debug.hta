
<HTML>
<HEAD>

<META http-equiv="Content-Language" content="en-us">
<!--<META http-equiv="Content-Type" content="text/html; charset=windows-1252"> -->
<META http-equiv="Content-Type" content="text/html; charset=UNICODE">

<SCRIPT language="JScript">

// This prevents resizeTo from flashing on HTA invoke
// This must be done before the hta tag

window.resizeTo(1,1);
window.moveTo(-1500,1);

</SCRIPT>

<HTA:APPLICATION ID="oHTA"
	applicationName="HUI Debug"
	border="thick"
	borderStyle="raised"
	caption="yes"
	icon=".\data\pics\icon.ico"
	CommandLine=""
	maximizeButton="yes"
	minimizeButton="yes"
	contextMenu="yes"
	innerBorder="no"
	selection="yes"
	showInTaskbar="yes"
	singleInstance="no"
	sysMenu="yes"
	navigable="yes"
	scroll="yes"
	scrollFlat="no"
	windowState="normal">

<TITLE>HUI Debug</title>

<script language="JScript.Encode" src="./code/scripts/hta-loader.js" type="text/javascript"></script>
<script language="JScript.Encode" src="./code/scripts/hta.js" type="text/javascript"></script>

<STYLE type="text/css">
@charset "Windows-1251";
@import url(./code/styles/hta.css);

</STYLE>

<script type="text/javascript" language="JScript.Encode">

function hta_onload(){
	try{
		hta_loader_load(true,"library/HUI");
		hta_position(window.screen.width,window.screen.height);
		//hta_position(1024,window.screen.height*0.9);
	}
	catch(e){
		alert("ERROR::hta_onload(): " + e.description)
	}
}

function hta_onunload(){
	try{
		document.title = "..unloading"

	}
	catch(e){
		alert("ERROR::hta_onunload(): " + e.description)
	}
}

function hta_position(iWidth,iHeight){
	try{
		//if(document.body) document.body.style.visibility = "hidden"
		var iLeft = (window.screen.width-iWidth)/2;
		var iTop = (window.screen.height-iHeight)/2;
		window.moveTo(iLeft,iTop);
		window.resizeTo(1,1);
		window.resizeTo(iWidth,iHeight);
		window.self.focus()
		//if(document.body) document.body.style.visibility = "visible"
		//document.recalc(true);
	}
	catch(e){
		alert("ERROR::hta_position() " + e.description);
	}
}

</script>

</HEAD>
<BODY scroll="auto" Onload="setTimeout('hta_onload()',0,'JavaScript')" onunload="hta_onunload();">

<div id="oDivBody">

<div id="oDivHome"></DIV>


<DIV id="oDivDebug" class="mba-div-body">
<form name="oFormDebug">
<table width="100%" align="center" border="0" cellspacing="0" cellpadding="0">
<tr>
	<td width="15">&nbsp;</td>
	<td width="75" class="bg-line">&nbsp;</td>
	<td width="15">&nbsp;</td>
	<td valign="top">
	
	<div class="hui-panel-frame" id="oDivDebugPanel">
	<div class="hui-panel-panes">	
	
	<div class="hui-panel-pane" title="Console">
	
	<fieldset>
	<legend>DHTML</legend>
	<div>
	<br>
	<textarea name="myeval" style="width:100%;height:250px;border-width;1;background-color:#EEEEEE" rows=5 wrap="off"></textarea>
	<input type="button" onclick="eval(this.form.myeval.value)" value="Evalutate" class="cInput2">
	</div>
	</fieldset>	
	
	<fieldset>
	<legend>Functions</legend>
	<div>
	<input size="50" name="jscount" type="text" style="width:100%;"><br>
	<input type="button" onclick="this.form.t_log.innerText=hta_JSFunctions();oDivBody.doScroll('down')" value="Count JS Functions" class="cInput2">
	</div>
	</fieldset>	
	
	<fieldset>
	<legend>Encoding</legend>
	<div>
	<br>
	<input size="50" name="encode" type="text" style="width:100%;"><br>
	<input type="button" onclick="this.form.t_log.innerText=htm_encode(this.form.encode.value);oDivBody.doScroll('down')" value="Encode" class="cInput2">
	</div>
	</fieldset>
	
	</div>		
	<div class="hui-panel-pane" title="Logs">
	
	<fieldset>
	<legend>Log</legend>
	<div>
	<textarea name="t_log" style="width:100%;height:250px;border-width:0;" cols="60"> </textarea>
	</div>
	</fieldset>

	<fieldset>
	<legend>Err</legend>
	<div>
	<textarea name="t_error" style="width:100%;height:200px;border-width:0;" cols="60"> </textarea>
	</div>
	</fieldset>
	
	</div>
	<div class="hui-panel-pane" title="DOM">
	
	<fieldset>
	<legend>HTML Element</legend>
	<div>
	<input size="50" name="element" type="text" style="width:100%;" value=""><br>
	<input name="element_action" type="button" class="cInput2" value="Evaluate" onclick="eval(this.form.element.value)"> &nbsp;
	<input name="element_action" type="button" class="cInput2" value="Clear" onclick="oDivDebugElement.innerHTML=''"><br><br>
	<br><br>
	<div id="oDivDebugElement"></div>
	</div>
	</fieldset>
	
	</div>
	<div class="hui-panel-pane" title="Annet">
	
	<textarea name="myeval2" style="width:100%;border-width:1;background-color:#EEEEEE" rows="20" wrap="off"></textarea>
	
	</div>
	
	</div></div>
	
	</td>
	<td width="15">&nbsp;</td>
	<td width="200" class="bg-line" valign="top" align="center">&nbsp;</td>
	<td width="15">&nbsp;</td>
</tr>
</table>
</form>
</DIV>


</DIV>

</body>
</html>
